{
  "version": "1.0.0",
  "configurations": [
    {
      "name": "Python",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "JS",
      "program": "${file}",
      "cwd": "${workspaceFolder}",
      "outputCapture" : "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "TS",
      "program": "${file}",
      "preLaunchTask": "tsc: watch - server/tsconfig.json",
      "outFiles": ["${workspaceFolder}/server/dist/**/*.js"]
    },
  ]
}
